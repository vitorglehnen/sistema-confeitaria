program Project1;

uses
  Vcl.Forms,
  Unit1 in 'Unit1.pas' {formMain},
  UnitCadastrarNovoPedido in 'UnitCadastrarNovoPedido.pas' {formCadastrarNovoPedido},
  UnitPedidos in 'UnitPedidos.pas' {Form3},
  UnitEditCardapio in 'UnitEditCardapio.pas' {Form4};

{$R *.res}

begin
  Application.Initialize;
  Application.MainFormOnTaskbar := True;
  Application.CreateForm(TformMain, formMain);
  Application.CreateForm(TformCadastrarNovoPedido, formCadastrarNovoPedido);
  Application.CreateForm(TForm3, Form3);
  Application.CreateForm(TForm4, Form4);
  Application.Run;
end.
